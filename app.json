{
  "expo": {
    "name": "LulaDefyReactNative",
    "slug": "LulaDefyReactNative",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "lula",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow Lula to scan secure QR tokens to begin onboarding."
      },
      "bundleIdentifier": "com.anonymous.LulaDefyReactNative"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA"
      ],
      "package": "com.anonymous.LulaDefyReactNative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "vendorBaseUrl": "https://api.vendor.example",
      "relayBaseUrl": "https://relay.vendor.example"
    }
  }
}
